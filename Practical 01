package com.mycompany.practical1;
public class Practical1 {
public static void main(String[] args) {
int x;
for(x=0;x<5;x++)
{
System.out.println("Executing Loop "+x);
}
}
}
package com.mycompany.practical1;
public class Practical1 ;
public static void main(String[] args) {
int x = 0;
while(x < 5)
{
System.out.println("Executing Loop "+x);
x++;
}
}
}
